<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- data service bean -->
	<bean id="eventService" class="entity.service.impl.EventServiceImpl" scope="prototype" />
	
	<!-- composer bean -->
	<bean id="indexComposer" class="web.core.controller.IndexComposer" scope="prototype" />
	<bean id="eventListComposer" class="web.event.controller.EventListComposer" scope="prototype" />
</beans>
